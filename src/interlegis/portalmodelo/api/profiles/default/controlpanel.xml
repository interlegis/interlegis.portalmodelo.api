<?xml version="1.0"?>
<object name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="interlegis.portalmodelo.api"
    purge="False">
  <configlet title="Portal Modelo Settings"
      action_id="portalmodelo"
      appId="interlegis.portalmodelo.api"
      category="Products"
      condition_expr=""
      url_expr="string:${portal_url}/@@portalmodelo-settings"
      icon_expr="string:${portal_url}/++resource++interlegis.portalmodelo.api/interlegis_icon.png"
      visible="True"
      i18n:attributes="title">
    <permission>Manage portal</permission>
  </configlet>
</object>
